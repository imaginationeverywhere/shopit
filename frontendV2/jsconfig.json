{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "actions/*": ["src/actions/*"],
      "actions": ["src/actions"],
      "reducers/*": ["src/reducers/*"],
      "utils/*": ["src/utils/*"],
      "utils": ["src/utils"],
      "hooks/*": ["src/hooks/*"],
      "pages/*": ["src/components/pages/*"],
      "admin/*": ["src/components/admin/*"]
    }
  },
  "exclude": ["dist", "node_modules", "config"]
}
